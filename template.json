{
    "version": "v1",
    "name": "VEX Protocol",
    "description": "The trust layer for AI agents - adversarial verification, temporal memory, and cryptographic proofs.",
    "services": {
        "vex-api": {
            "source": {
                "repo": "provnai/vex"
            },
            "variables": {
                "VEX_JWT_SECRET": "${{ secret }}",
                "DATABASE_URL": "sqlite:///data/vex.db?mode=rwc",
                "VEX_ENV": "production",
                "DEEPSEEK_API_KEY": ""
            }
        }
    }
}